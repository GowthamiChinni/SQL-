SELECT * FROM EMPDATA
WHERE EMPNAME LIKE '%REE%';
SELECT * FROM EMPDATA
WHERE EMPDEPT LIKE '_ECH';
SELECT * FROM EMPDATA
WHERE EMPNAME LIKE '[R]%';
SELECT * FROM EMPDATA
WHERE EMPNAME LIKE '[N-Z]%';
SELECT * FROM EMPDATA
WHERE  EMPNAME LIKE 'a__%';
SELECT * FROM EMPDATA
WHERE EMPNAME LIKE '_r%';
SELECT * FROM EMPDATA
WHERE EMPNAME LIKE 'RUHI';
